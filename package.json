{
  "name": "exiftool-vendored",
  "version": "4.15.0",
  "description": "Efficient, cross-platform access to ExifTool",
  "main": "./dist/ExifTool.js",
  "types": "./dist/ExifTool.d.ts",
  "scripts": {
    "clean": "rimraf lib dist coverage .nyc_output",
    "precompile": "run-s clean",
    "compile": "tsc",
    "preupdate": "run-s compile",
    "update": "node dist/update/update.js",
    "premktags": "run-s compile",
    "mktags": "node dist/update/mktags.js",
    "prettier": "prettier --write src/**/*.ts",
    "pretest": "run-s compile",
    "test": "nyc mocha --opts .mocha.opts",
    "report": "run-s test && nyc report -r html && opn coverage/index.html"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mceachen/exiftool-vendored.js.git"
  },
  "engines": {
    "node": ">= 4"
  },
  "keywords": [
    "ExifTool",
    "EXIF",
    "JPEG",
    "TIFF",
    "RAW",
    "ORF",
    "DNG",
    "XMP",
    "MakerNote",
    "Metadata"
  ],
  "author": "Matthew McEachen <matthew-exiftoolvendored@mceachen.org>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mceachen/exiftool-vendored.js/issues"
  },
  "nyc": {
    "exclude": [
      "**/*.spec.?s",
      "**/update/**"
    ]
  },
  "homepage": "https://mceachen.github.io/exiftool-vendored.js/",
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/chai-as-promised": "^7.1.0",
    "@types/mocha": "^2.2.48",
    "@types/node": "^9.4.0",
    "@types/pify": "^3.0.0",
    "@types/rimraf": "^2.0.2",
    "@types/semver": "^5.5.0",
    "@types/tmp": "^0.0.33",
    "@types/xmldom": "^0.1.29",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "cp-file": "^5.0.0",
    "decompress-zip": "^0.3.0",
    "globule": "^1.2.0",
    "mocha": "^5.0.0",
    "np": "^2.19.0",
    "npm-run-all": "^4.1.2",
    "nyc": "^11.4.1",
    "opn-cli": "^3.1.0",
    "pify": "^3.0.0",
    "prettier": "^1.10.2",
    "progress": "^2.0.0",
    "rimraf": "^2.6.2",
    "semver": "^5.5.0",
    "source-map-support": "^0.5.3",
    "tar-fs": "^1.16.0",
    "tmp": "^0.0.33",
    "typescript": "^2.7.1",
    "xmldom": "^0.1.27",
    "xpath": "^0.0.27"
  },
  "dependencies": {
    "batch-cluster": "^1.8.0"
  },
  "optionalDependencies": {
    "exiftool-vendored.exe": "10.76.0",
    "exiftool-vendored.pl": "10.76.0"
  }
}
