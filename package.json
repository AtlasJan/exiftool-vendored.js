{
  "name": "exiftool-vendored",
  "version": "6.3.0",
  "description": "Efficient, cross-platform access to ExifTool",
  "main": "./dist/ExifTool.js",
  "types": "./dist/ExifTool.d.ts",
  "homepage": "https://exiftool-vendored.js.org/",
  "bugs": {
    "url": "https://github.com/mceachen/exiftool-vendored.js/issues"
  },
  "scripts": {
    "clean": "rimraf lib dist coverage .nyc_output",
    "precompile": "npm run clean",
    "compile": "tsc",
    "preupdate": "npm run compile",
    "update": "node dist/update/update.js",
    "premktags": "npm run compile",
    "mktags": "node dist/update/mktags.js",
    "prettier": "prettier --write src/*.ts src/**/*.ts",
    "pretest": "npm run compile",
    "test": "nyc mocha --opts .mocha.opts",
    "report": "npm run test && nyc report -r html",
    "docs:1": "typedoc --options ./.typedoc.js",
    "docs:2": "echo exiftool-vendored.js.org > docs/CNAME",
    "docs:3": "cp .serve.json docs/serve.json",
    "docs:4": "touch docs/.nojekyll",
    "predocs": "bash -c 'mkdir -p docs' && run-s docs:*",
    "docs": "yarn serve docs"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mceachen/exiftool-vendored.js.git"
  },
  "engines": {
    "node": ">= 6"
  },
  "keywords": [
    "ExifTool",
    "EXIF",
    "JPEG",
    "TIFF",
    "RAW",
    "ORF",
    "DNG",
    "XMP",
    "MakerNote",
    "Metadata"
  ],
  "author": "Matthew McEachen <matthew-exiftoolvendored@mceachen.org>",
  "license": "MIT",
  "nyc": {
    "exclude": [
      "**/*.spec.?s",
      "**/update/**"
    ]
  },
  "devDependencies": {
    "@types/chai": "^4.1.6",
    "@types/chai-as-promised": "^7.1.0",
    "@types/globule": "^1.1.3",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.11.6",
    "@types/pify": "^3.0.2",
    "@types/progress": "^2.0.1",
    "@types/rimraf": "^2.0.2",
    "@types/tmp": "^0.0.33",
    "@types/xmldom": "^0.1.29",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "cp-file": "^6.0.0",
    "decompress-zip": "^0.3.1",
    "globule": "^1.2.1",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.1.3",
    "nyc": "^13.0.1",
    "pify": "^4.0.0",
    "prettier": "^1.14.3",
    "progress": "^2.0.0",
    "rimraf": "^2.6.2",
    "serve": "^10.0.2",
    "source-map-support": "^0.5.9",
    "tar-fs": "^1.16.3",
    "tmp": "^0.0.33",
    "typedoc": "^0.12.0",
    "typescript": "^3.1.2",
    "xmldom": "^0.1.27",
    "xpath": "^0.0.27"
  },
  "dependencies": {
    "batch-cluster": "^4.3.0"
  },
  "optionalDependencies": {
    "exiftool-vendored.exe": "11.13.0",
    "exiftool-vendored.pl": "11.13.0"
  }
}
