{
  "name": "exiftool-vendored",
  "version": "0.1.0",
  "description": "Efficient, cross-platform access to ExifTool",
  "main": "./lib/exiftool.js",
  "scripts": {
    "clean": "rimraf lib",
    "lint": "tslint src/**/*.ts",
    "fmt": "bash -c 'tsfmt -r src/**/*.ts'",
    "compile": "tsc",
    "preupdate": "run-s clean lint compile",
    "update": "node lib/update/update.js",
    "premktags": "tsc",
    "mktags": "node lib/update/mktags.js",
    "pretest": "tsc",
    "test": "mocha lib/**/*.spec.js",
    "cleantest": "run-s clean lint fmt pdm test",
    "pdm": "platform-dependent-modules",
    "postinstall": "npm run pdm",
    "publish": "run-s cleantest"
  },
  "files": [
    "lib"
  ],
  "directories": {
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mceachen/exiftool-vendored.git"
  },
  "engines": {
    "node": ">= 6.4.0"
  },
  "keywords": [
    "ExifTool",
    "EXIF",
    "JPEG",
    "TIFF",
    "RAW",
    "ORF",
    "DNG",
    "XMP",
    "MakerNote",
    "Metadata"
  ],
  "author": "Matthew McEachen <matthew-exiftoolvendored@mceachen.org>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mceachen/exiftool-vendored/issues"
  },
  "homepage": "https://github.com/mceachen/exiftool-vendored#readme",
  "config": {
    "platformDependentModules": {
      "linux": [
        "exiftool-vendored.pl@10.31.0"
      ],
      "darwin": [
        "exiftool-vendored.pl@10.31.0"
      ],
      "win32": [
        "exiftool-vendored.exe@10.31.0"
      ]
    }
  },
  "devDependencies": {
    "@types/chai": "^3.4.34",
    "@types/chai-as-promised": "0.0.29",
    "@types/jsdom": "^2.0.29",
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.45",
    "@types/rimraf": "0.0.28",
    "@types/xmldom": "^0.1.28",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "decompress-zip": "^0.3.0",
    "globule": "^1.0.0",
    "mocha": "^3.1.2",
    "npm-run-all": "^3.1.1",
    "rimraf": "^2.5.4",
    "tar-fs": "^1.14.0",
    "tslint-config-standard": "^1.5.0",
    "typescript": "^2.0.3",
    "typescript-formatter": "^3.1.0",
    "xmldom": "^0.1.22",
    "xpath": "0.0.23"
  },
  "dependencies": {
    "platform-dependent-modules": "0.0.14"
  }
}
